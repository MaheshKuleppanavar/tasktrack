<%if(success&&success.length){%>
    <div class="alert alert-success alert-dismissible fade show col-sm-sm-6 col-10 offset-1 col-10 offset-1  offset-sm-sm-3 mt-4" role="alert">
        <strong><%= success%></strong>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
<%}%>
<%if(error&&error.length){%>
    <div class="alert alert-danger alert-dismissible fade show col-sm-6 col-10 offset-1  offset-sm-3 mt-4" role="alert">
        <strong><%= error%></strong>  
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
<%}%>
